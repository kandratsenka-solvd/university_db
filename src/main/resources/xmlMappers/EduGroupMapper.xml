<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.IEduGroupMapper">

    <select id="getAll" resultMap="groupResultMap">
        SELECT *
          FROM edu_group
    </select>

    <resultMap id="groupResultMap" type="models.EduGroup">
        <id property="eduGroupId" column="edu_group_id" />
        <result property="groupName" column="group_name" />
        <result property="degreeId" column="degree_id" />
        <result property="departmentId" column="department_id" />
    </resultMap>

</mapper>
